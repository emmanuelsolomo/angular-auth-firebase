{
  "name": "ngx-auth-firebaseui",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Demo app for ngx-auth-firebaseui",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:client-and-server-bundles": "ng build --prod --preserve-symlinks &&  ng run ngx-auth-firebaseui:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "deploy:firebase": "npm run build:prod && npm run build:doc && firebase deploy",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "lint": "ng lint",
    "test": "ng test",
    "postinstall": "npm run link",
    "link": "npm link ngx-auth-firebaseui",
    "start": "ng serve --port 4004",
    "serve:prod": "ng serve --prod",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/pages": "^2.1.0",
    "@angular/animations": "8.2.9",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "8.2.9",
    "@angular/compiler": "8.2.9",
    "@angular/core": "8.2.9",
    "@angular/fire": "^5.3.0",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "8.2.9",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "8.2.9",
    "@angular/platform-browser-dynamic": "8.2.9",
    "@angular/platform-server": "8.2.9",
    "@angular/router": "8.2.9",
    "@angular/service-worker": "8.2.9",
    "@ng-bootstrap/ng-bootstrap": "^5.1.5",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angulartics2": "^7.5.2",
    "bootstrap": "^4.4.1",
    "core-js": "^2.6.11",
    "firebase": "^6.6.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.17.1",
    "include-media": "^1.4.9",
    "material-design-icons": "^3.0.1",
    "ngx-clipboard": "^12.3.0",
    "ngx-highlightjs": "^3.0.3",
    "ngx-markdown": "^8.2.1",
    "rxjs": "^6.5.4",
    "svg-country-flags": "^1.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "8.3.8",
    "@angular/compiler-cli": "8.2.9",
    "@angular/language-service": "8.2.9",
    "@angularclass/hmr": "~2.1.3",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^10.17.13",
    "codelyzer": "^5.2.1",
    "express": "^4.17.1",
    "firebase-tools": "^7.11.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "~5.4.1",
    "reflect-metadata": "~0.1.10",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "~3.5.3",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "~1.7.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@types/jasmine"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
